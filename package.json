{
  "name": "frontend-seed-4",
  "version": "1.2.0",
  "description": "A basic HTML starter template for Bootstrap 4",
  "keywords": [
    "css",
    "sass",
    "html",
    "responsive",
    "theme",
    "template"
  ],
  "homepage": "https://github.com/trungnghia112/frontend-seed-4",
  "author": "trungnghia112 <trungnghia112@gmail.com>",
  "bugs": {
    "url": "https://github.com/trungnghia112/frontend-seed-4/issues",
    "email": "trungnghia112@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/trungnghia112/frontend-seed-4"
  },
  "scripts": {
    "start": "gulp serve",
    "prettier": "prettier ./**/*.{ts,json,md,scss} --write",
    "clean": "rm -rf ./dist/ && rm -rf ./.tmp/ && rm -rf ./css/",
    "build": "npm run clean && npm run prettier && gulp build"
  },
  "engines": {
    "node": ">=6.9.0",
    "npm": ">=3.10.3"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ]
    }
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "browser-sync": "^2.18.6",
    "del": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-babel": "^6.1.2",
    "gulp-cache": "^0.4.5",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.2",
    "gulp-eslint": "^3.0.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^3.0.1",
    "gulp-load-plugins": "^1.2.4",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.0.0",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^2.2.0",
    "gulp-uglify": "^2.0.0",
    "prettier": "1.11.1"
  },
  "dependencies": {
    "bootstrap": "^4.0.0",
    "jquery": "^3.2.1",
    "popper.js": "^1.12.9"
  }
}
